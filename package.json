{
  "private": true,
  "scripts": {
    "start": "parcel --out-dir docs src/index.html",
    "build": "parcel build --out-dir docs src/index.html",
    "tsc:watch": "tsc --noEmit --watch",
    "prettier:check": "prettier --check 'src/**/*.{html,ts,scss}'",
    "prettier:write": "prettier --write 'src/**/*.{html,ts,scss}'",
    "elm-format:check": "elm-format --validate src",
    "elm-format:write": "elm-format --yes src"
  },
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.3",
    "elm-hot": "^1.1.4",
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.5.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.2",
    "typescript": "^3.8.3"
  }
}
